@model PDFTOXLS.Models.Group

    <div class="row">
        <div class="col-sm-12 col-md-12 col-xs-12">
            
  <div class="col-xs-12 clearfix">
      <div class="paddingboth topbtmpadding clearfix">
          <h2 class="font-md">Create Group</h2>
<form  method="post" action="/Group/Save"  id="validate-form" style="margin-bottom: 0px !important;">
      @Html.HiddenFor(m => m.ID, new { @Value = @ViewBag.Id })
   @*  @Html.HiddenFor(m => m.is_clnt, new { @Value =true })
     @Html.HiddenFor(m=>m.TASK_NO, new { @class="form-control" })*@

     <div class="row">
        <div class="col-xs-12 col-sm-4 col-md-4">
    <div class="form-group">
    <label >Group Title <span class="required">*</span>:</label>
  
      @Html.TextBoxFor(m=>m.GroupTitle, new { @class="form-control input-sm" })
      <div>@Html.ValidationMessageFor(model => model.GroupTitle)</div>
   
  </div>
  </div>
   <div class="col-xs-12 col-sm-4 col-md-4">
    <div class="form-group">
    <label>Description</label>
    
    
      @Html.TextBoxFor(m=>m.Description, new { @class="form-control input-sm" })
      <div>@Html.ValidationMessageFor(model => model.Description)</div>
    
     </div>
  </div>

<div class="col-xs-12 col-sm-4 col-md-4">
<div class="form-group">
    <label>Primary Consultant<span class="required">*</span>:</label>
    
    @*     @Html.TextBoxFor(m=>m.PrimaryConsultant, new { @class="form-control input-sm" })
      <div>@Html.ValidationMessageFor(model => model.PrimaryConsultant)</div>*@
      @Html.DropDownList("PrimaryConsultant", (IEnumerable<SelectListItem>)ViewBag.consultants,null, new { @class="form-control input-sm" })
  </div>
</div>


    </div>

    
   
        
   
        <input type="submit" class="btn btn-xs btn-primary" name="btnSubmit" value="Update" />

     <a class="btn  btn-xs btn-default" href="@Url.Action("List", "Group")">Cancel</a>
    


 </form>
          </div>
      </div>
        </div>
        @*<div class="col-sm-2 col-md-2 col-xs-12 custom borderleft">
           <div class="sidebar topbtmpadding">
          This is inside a relative positioned z-index: 1 div
          </div>

        </div>*@
    </div>


